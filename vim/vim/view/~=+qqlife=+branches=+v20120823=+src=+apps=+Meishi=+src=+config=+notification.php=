imap <buffer> D <Plug>ZenCodingBalanceTagOutwardInsert
imap <buffer> d <Plug>ZenCodingBalanceTagInwardInsert
imap <buffer> ; <Plug>ZenCodingExpandWord
imap <buffer>  <Plug>ZenCodingExpandAbbr
inoremap <buffer> { {}O
let &cpo=s:cpo_save
unlet s:cpo_save
